<launch>
    <arg name="module_name" default="HexPercept2" />

    <!-- Gazebo -->
    <include file="$(find hexapod_elevation_mapping)/launch/gazebo.launch" >
        <arg name="module_name" value="$(arg module_name)"/>
    </include>

    <!-- Rviz -->
    <node name="hex_elemap_rviz" pkg="rviz" type="rviz" args="-d $(find hexapod_elevation_mapping)/config/rviz/elspider_air_mapping_sim.rviz" required="true"/>
</launch>
